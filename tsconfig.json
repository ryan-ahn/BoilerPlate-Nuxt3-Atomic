{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@apis/*": ["apis/*"],
      "@assets/*": ["assets/*"],
      "@common/*": ["common/*"],
      "@components/*": ["components/*"],
      "@containers/*": ["containers/*"],
      "@interface/*": ["interface/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@plugins/*": ["plugins/*"],
      "@router/*": ["router/*"],
      "@store/*": ["store/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "extends": "./.nuxt/tsconfig.json"
}
